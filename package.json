{
  "name": "recipeak-api",
  "version": "0.0.1",
  "description": "The first recipeak api for thinkful capstone",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "seed:api": "psql -U recipeak -d recipeak-api -f ./seeds/seed.recipes_table.sql",
    "unseed:api": "psql -U recipeak -d recipeak-api -f ./seeds/trunc.recipes_table.sql",
    "unseed:test": "psql -U recipeak -d recipeak-api-test -f ./seeds/trunc.recipes_table.sql",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master",
    "devlog": "git add README.md",
    "postdevlog": "git commit -m 'update log'"
  },
  "keywords": [],
  "author": "Alex Fukui <alexander.fukui@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "uuid": "^3.3.3",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  }
}
